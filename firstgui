-- Load Rayfield Library
local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Rayfield/main/source'))()

-- Create Rayfield Window
local Window = Rayfield:CreateWindow({
    Name = "Teleporter",
    LoadingTitle = "Teleport GUI",
    LoadingSubtitle = "Powered by Rayfield",
    ConfigurationSaving = {
        Enabled = false -- Disable saving for simplicity
    },
    Discord = {
        Enabled = false -- No Discord integration for this use case
    }
})

-- Game ID for DOORS
local doorsGameId = 6516141723
local teleportService = game:GetService("TeleportService")

-- Add a Button to the Rayfield Window
Window:CreateButton({
    Name = "tp",
    Callback = function()
        teleportService:Teleport(doorsGameId)
    end
})
